#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules/@mark.probst/typescript-json-schema/bin/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules/@mark.probst/typescript-json-schema/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules/@mark.probst/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules/@mark.probst/typescript-json-schema/bin/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules/@mark.probst/typescript-json-schema/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules/@mark.probst/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/@mark.probst+typescript-json-schema@0.55.0/node_modules:/mnt/d/Documentos/GitHub/estudos-typescript/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/typescript-json-schema" "$@"
else
  exec node  "$basedir/../../bin/typescript-json-schema" "$@"
fi
